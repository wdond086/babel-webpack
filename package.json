{
  "name": "babel-webpack",
  "version": "1.0.0",
  "description": "Practice setting up Babel and Webpack to compile and transpile ES6+ code",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-dev": "rm -rf dist && webpack --progress --mode development",
    "build-prod": "rm -rf dist && webpack --progress --mode production",
    "start": "node ./dist/app.js",
    "babel": "./node_modules/.bin/babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wdond086/babel-webpack.git"
  },
  "keywords": [
    "Babel",
    "Webpack",
    "Node"
  ],
  "author": "Wilfried Dondji",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wdond086/babel-webpack/issues"
  },
  "homepage": "https://github.com/wdond086/babel-webpack#readme",
  "dependencies": {
    "chalk": "^2.4.2",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-stage-0": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^3.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.39.3",
